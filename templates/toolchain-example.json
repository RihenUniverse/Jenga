{
  "name": "my-custom-gcc",
  "type": "gcc",
  "description": "Custom GCC 13 toolchain for Linux",
  "target": {
    "os": "Linux",
    "arch": "x86_64",
    "env": "gnu"
  },
  "target_triple": "x86_64-unknown-linux-gnu",
  "sysroot": "",
  "cc": "/usr/bin/gcc-13",
  "cxx": "/usr/bin/g++-13",
  "linker": "/usr/bin/g++-13",
  "ar": "/usr/bin/ar",
  "cflags": [
    "-march=native",
    "-mtune=native"
  ],
  "cxxflags": [
    "-std=c++20",
    "-march=native",
    "-mtune=native"
  ],
  "ldflags": [
    "-fuse-ld=gold"
  ],
  "arflags": [
    "rcs"
  ],
  "defines": [
    "MY_CUSTOM_DEFINE=1"
  ],
  "include_dirs": [],
  "lib_dirs": []
}
